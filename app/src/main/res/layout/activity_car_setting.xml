<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CarSettingActivity">

    <RadioButton
        android:id="@+id/radioCar4"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:text="Car 4"
        app:layout_constraintStart_toEndOf="@+id/textViewCar4"
        tools:layout_editor_absoluteY="189dp" />

    <RadioButton
        android:id="@+id/radioCar3"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:text="Car 3"
        app:layout_constraintStart_toEndOf="@+id/textViewCar3"
        tools:layout_editor_absoluteY="144dp" />

    <RadioButton
        android:id="@+id/radioCar2"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:text="Car 2"
        app:layout_constraintStart_toEndOf="@+id/textViewCar2"
        tools:layout_editor_absoluteY="99dp" />

    <TextView
        android:id="@+id/textViewCar4"
        android:layout_width="229dp"
        android:layout_height="21dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:text="Year: none Make: none Model: none"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="189dp" />

    <TextView
        android:id="@+id/textViewCar3"
        android:layout_width="229dp"
        android:layout_height="21dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginBottom="24dp"
        android:text="Year: none Make: none Model: none"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintBottom_toTopOf="@+id/textViewCar4"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewCar"
        android:layout_width="229dp"
        android:layout_height="21dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginBottom="24dp"
        android:text="Year: none Make: none Model: none"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintBottom_toTopOf="@+id/textViewCar2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewCar2"
        android:layout_width="229dp"
        android:layout_height="21dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginBottom="24dp"
        android:text="Year: none Make: none Model: none"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintBottom_toTopOf="@+id/textViewCar3"
        app:layout_constraintStart_toStartOf="parent" />

    <RadioButton
        android:id="@+id/radioCar1"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:text="Car 1"
        app:layout_constraintStart_toEndOf="@+id/textViewCar"
        tools:layout_editor_absoluteY="54dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="Save"
        app:layout_constraintTop_toBottomOf="@+id/textViewCar4"
        tools:layout_editor_absoluteX="147dp" />

    <RadioGroup
        android:layout_width="66dp"
        android:layout_height="154dp"
        tools:layout_editor_absoluteX="305dp"
        tools:layout_editor_absoluteY="55dp" />

    <android.support.design.widget.TextInputLayout
        android:id="@+id/editText1a"
        android:layout_width="79dp"
        android:layout_height="23dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintStart_toStartOf="@+id/textViewCar"
        tools:layout_editor_absoluteY="53dp">

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="35dp" />
    </android.support.design.widget.TextInputLayout>

</android.support.constraint.ConstraintLayout>